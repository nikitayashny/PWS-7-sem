<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <system.serviceModel>
    <services>
      <service name="PWS_Lab5.WCFSiplex">
        <endpoint address="" binding="basicHttpBinding" contract="PWS_Lab5.IWCFSiplex">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
		  <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
		  <endpoint address="net.tcp://localhost:8739/WCF/WCFSiplex/" binding="netTcpBinding" name="tcpEndpoint" contract="PWS_Lab5.IWCFSiplex" />
		<host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/WCF/WCFSiplex/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

</configuration>
